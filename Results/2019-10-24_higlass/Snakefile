import os.path as path

rule all:
    input:
        path.join("Data", "hg38", "gene-annotations-hg38.beddb"),


rule make_higlass_hg38_annotations:
    input:
        script = "make-hg38-annotations.sh",
        data = "hg38.chrom.sizes",
        exon_script = "exonU.py",
    output:
        path.join("Data", "hg38", "gene-annotations-hg38.beddb"),
    shell:
        "sh {input.script}"
